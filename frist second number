#include<iostream>
using namespace std;
int main(){

	int n1;
    int n2;
	int sum=0;
	cout << "input first number : ";
	cin >> n1;
	cout << "input second number : ";
	cin >> n2;
	for(int i=1;i<=n2;i++)
	{
		cout <<n1<<"*"<<i<<"="<<n1*i<<endl;
		sum = sum+(n1*i);

	}
	cout << "Total : " << sum << endl;
	
	return(0);

	
		

}
